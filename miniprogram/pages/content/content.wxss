.all{
  background-color: greenyellow;
  height: 100vh;
  width: 100vw;
}
.dk{
    width: 80%;
    margin: 40rpx auto;
    height: 80rpx;
    justify-content: center;
    align-items: center;
    display: flex;
    border-radius: 38rpx;
    box-shadow: 7px 7px 20px #ececec;
    background: linear-gradient(45deg, yellow,green);
    color:burlywood;
    box-sizing: border-box;
}
.delete{
  width: 80%;
  margin: 40rpx auto;
  height: 80rpx;
  justify-content: center;
  align-items: center;
  display: flex;
  position: absolute;
  bottom: 30rpx;
  left: 80rpx;
  border-radius: 38rpx;
  box-shadow: 7px 7px 20px #ececec;
  background: linear-gradient(45deg, yellow,red);
  color:burlywood;
  box-sizing: border-box;
}

.feel{
  width: 80%;
  height: 500rpx;
  margin: 40rpx auto;
  display: flex;
  border-radius: 38rpx;
  background-color: green;
  flex-wrap: wrap;
  justify-content: center;
  box-shadow: 7px 7px 20px yellow;
}
.title{
  display: flex;
  justify-content: center;
  margin: 0 auto;
  width: 100%;
  height: 50rpx;
  box-shadow: 7px 7px 20px #ececec;
}
