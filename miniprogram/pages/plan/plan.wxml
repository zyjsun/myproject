<swiper indicator-color="white"  indicator-dots="true"   interval="true" autoplay="true" interval="1000"  duration="2000">
  <block wx:for="{{imgurl}}" wx:key="index">
  <swiper-item >
    <image src="{{item}}" mode="widthFix" class="slide-image" width="100%"></image>
  </swiper-item>
</block>
</swiper>
<view class="tips">
  <van-icon name="fire" />
    宝剑锋从磨砺出，梅花香自苦寒来
</view>

<view class="addplan" bindtap="add">+添加计划</view>
  
    <view class="item" wx:for="{{obj}}" bindtap="jump"  data-index="{{index}}" wx:key="index">
        <image src="../../images/sleep.jpg" class="img"/>
        <view class="text">
        <text>{{item.plan}}</text>
        <text class="t">总打卡1天</text>
        </view>
        <view class="right">
        <view class="day">1天</view>
        <text>连续打卡</text>
        </view>
    </view>


